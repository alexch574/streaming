<h1>Create stream</h1>

<form action="/streams/create" method="POST">
<div>
    <label for="name">Name</label>
    <input
      type="name"
      name="name"
      placeholder="name"
      value="<%= typeof name != 'undefined' ? name : '' %>"
    />
  
</div>

  <button type="submit" class="btn btn-primary btn-block">
    Create
  </button>
</form>

